    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.10",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-scripts": "4.0.0",
    "uuid": "8.3.1"
  },
  "scripts": {
