{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": ["airbnb", "plugin:jsx-a11y/recommended", "prettier", "prettier/react"],
  "parser": "babel-eslint",
  "plugins": ["jsx-a11y", "prettier"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  }
}
